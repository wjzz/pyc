# x = 10
# if x % 2 == 0:
#     print 0
# else:
#     print 1
# """
[
    StmAssign("x", ArithLit(10)),
    StmIf(
        BoolBinop(
            ArithCmp.Eq,
            ArithBinop(
                ArithOp.Mod,
                ArithVar("x"),
                ArithLit(2)),
            ArithLit(0)),
        [
            StmPrint(ArithLit(0))
        ],
        [
            StmPrint(ArithLit(1))
        ]
    )
]